<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Package name="~org\.tikv\.(?!kvproto).*"/>
    <Not>
      <Package name="~org\.tikv\.common\.parser.*"/>
    </Not>
    <Not>
      <Or>
        <Bug pattern="EI_EXPOSE_REP"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
      </Or>
    </Not>
    <Rank value="1"/>
  </Match>
</FindBugsFilter>
